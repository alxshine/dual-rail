root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
indent_style = space
indent_size = 2

[Makefile]
indent_style = tab

[*.{h,hh,hpp,c,cc,cpp,cxx}]
indent_style = space
indent_size = 2

[*.{html,css,js}]
indent_style = space
indent_size = 2
